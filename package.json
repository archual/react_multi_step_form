{
  "name": "react-forms",
  "version": "1.0.0",
  "description": "React froms",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "start": "NODE_ENV=development webpack-dev-server",
    "test": "NODE_ENV=test node test.js --env=jsdom --watchAll",
    "test-coverage": "NODE_ENV=test node test.js --env=jsdom --watchAll --coverage"
  },
  "author": "Jim Mastrangelo",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.6.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-script": "^1.0.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "html-webpack-plugin": "^2.19.0",
    "imports-loader": "^0.6.5",
    "jest": "^23.4.1",
    "prop-types": "^15.6.1",
    "react-addons-perf": "^15.4.2",
    "react-addons-update": "^15.1.0",
    "redux-devtools": "^3.3.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.9",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0",
    "webpack-validator": "^2.1.2",
    "whatwg-fetch": "^1.0.0"
  },
  "dependencies": {
    "antd": "^3.3.0",
    "compose-function": "^3.0.3",
    "deep-equal": "^1.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "es6-promise": "^4.0.5",
    "google-map-react": "^0.24.0",
    "lodash": "^4.17.4",
    "logrocket": "^0.6.1",
    "parts": "0.0.1-alpha.3",
    "points-cluster": "^0.1.4",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-addons-update": "^15.1.0",
    "react-alert": "^2.3.0",
    "react-autosuggest": "^9.3.1",
    "react-ckeditor-wrapper": "^1.0.22",
    "react-dom": "^16.2.0",
    "react-dropzone-component": "^2.0.0",
    "react-redux": "^4.4.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-sortable-hoc": "^0.6.5",
    "react-test-renderer": "^16.4.1",
    "redux": "^3.5.2",
    "redux-form": "^6.8.0",
    "redux-thunk": "^2.1.0",
    "request": "^2.83.0",
    "whatwg-fetch": "^1.1.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/app/**/__tests__/**/*.js?(x)",
      "<rootDir>/app/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
